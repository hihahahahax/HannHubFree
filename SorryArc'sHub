menuButtonFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
menuButtonFrame.BackgroundTransparency = 0.2
menuButtonFrame.Active = true
menuButtonFrame.Parent = gui
makeDraggable(menuButtonFrame)

