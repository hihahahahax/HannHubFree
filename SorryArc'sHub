-- Nút teleport
local teleportButton = Instance.new("TextButton")
teleportButton.Size = UDim2.new(1, 0, 1, 0)
teleportButton.Text = "Teleport to Platform"
teleportButton.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
teleportButton.TextScaled = true
teleportButton.Parent = teleportMenu

-- Trạng thái teleport
local teleported = false
local originalCFrame = nil

teleportButton.MouseButton1Click:Connect(function()
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")

if not teleported then  
	originalCFrame = hrp.CFrame  
	hrp.CFrame = CFrame.new(platform.Position + Vector3.new(0, 5, 0))  
	teleportButton.Text = "Return to Original Position"  
	teleported = true  
else  
	if originalCFrame then  
		hrp.CFrame = originalCFrame  
	end  
	teleportButton.Text = "Teleport to Platform"  
	teleported = false  
end

end)

-- Toggle ẩn/hiện menu teleport
menuButton.MouseButton1Click:Connect(function()
teleportMenu.Visible = not teleportMenu.Visible
end)

-- ✅ Khi reset nhân vật, GUI sẽ biến mất
player.CharacterAdded:Connect(function()
if gui and gui.Parent then
gui:Destroy()
end
end)
  end
